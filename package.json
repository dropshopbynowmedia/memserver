{
  "name": "memserver",
  "version": "1.2.7",
  "type": "module",
  "description": "in-memory database/ORM and http mock server you can run in-browser and node environments. Built for large frontend teams, fast tests and rapid prototyping",
  "author": "Izel Nakri",
  "license": "MIT",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.4.0"
  },
  "bin": {
    "memserver": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/memserver.git"
  },
  "scripts": {
    "test": "node_modules/.bin/ava --verbose",
    "bin": "node --experimental-modules --experimental-repl-await cli.js",
    "build:browser": "rollup -c --environment BUILD:development",
    "build:model": "rollup -i ./lib/model.js -o model.js -f cjs",
    "build:response": "rollup -i ./lib/response.js -o response.js -f cjs",
    "build:memserver-cjs": "rollup -i ./lib/mem-server.js -o ./lib/mem-server-cjs.js -f cjs",
    "prepublish": "npm run build:model && npm run build:response && npm run build:memserver-cjs"
  },
  "dependencies": {
    "ansi-colors": "3.2.4",
    "ember-cli-string-utils": "^1.1.0",
    "fake-xml-http-request": "^2.0.0",
    "i": "^0.3.6",
    "jsdom": "^11.12.0",
    "pretender": "^3.0.1",
    "qs": "^6.7.0",
    "rollup": "1.12.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-virtual": "^1.0.1",
    "route-recognizer": "^0.3.4"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "fs-extra": "^8.0.1",
    "jquery": "^3.4.1",
    "mber": "^0.8.6",
    "sinon": "^7.3.2"
  },
  "ava": {
    "require": [
      "esm",
      "./disable-colors"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "volta": {
    "node": "12.4.0"
  }
}
